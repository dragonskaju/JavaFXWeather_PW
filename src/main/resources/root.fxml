<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.sda.openweather.controller.RootController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="408.0" prefWidth="689.0" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" layoutX="28.0" layoutY="45.0" prefHeight="27.0" prefWidth="117.0" style="&#10;" text="Podaj miasto" textAlignment="CENTER" textFill="#45ddda" wrapText="false">
                    <font>
                        <Font name="System Italic" size="18.0" />
                    </font>
                </Label>
                <TextField fx:id="city" layoutX="146.0" layoutY="46.0" />
                <Button fx:id="search" defaultButton="true" layoutX="329.0" layoutY="46.0" mnemonicParsing="false" onAction="#setCity" opacity="0.61" prefHeight="32.0" prefWidth="98.0" text="szukaj" textFill="#0faa17">
                    <effect>
                        <DropShadow color="#d51313" />
                    </effect>
                </Button>
                <Label fx:id="temperatura" layoutX="118.0" layoutY="130.0" prefHeight="18.0" prefWidth="194.0" text="Temperatura" />
                <Label fx:id="temperatura_odczuwalna" layoutX="118.0" layoutY="157.0" prefHeight="18.0" prefWidth="194.0" text="TEmperatura odczuwalna" />
                <Label fx:id="dodajTemperatura" layoutX="320.0" layoutY="130.0" prefHeight="18.0" prefWidth="194.0" text="0" />
                <Label fx:id="dodajTemperatura_odczuwalna" layoutX="320.0" layoutY="157.0" prefHeight="18.0" prefWidth="194.0" text="0" />
            <ImageView fx:id="obrazek" fitHeight="70.0" fitWidth="70.0" layoutX="111.0" layoutY="216.0" pickOnBounds="true" preserveRatio="true" />
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </AnchorPane>
    </children>
</VBox>
